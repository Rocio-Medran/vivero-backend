					"",
					"// Test for Content-Type header",
					"pm.test(\"Content-Type is application/json\", function () {",
					"    pm.response.to.have.header(\"Content-Type\");",
					"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/json\");",
					"});",
					"",
					"// Test for expected response structure (example: GET Productos con detalles)",
					"if (pm.request.url.path && pm.request.url.path.join(\"/\").includes(\"productos/detalles\")) {",
					"    let jsonData = pm.response.json();",
					"    pm.test(\"Response has success, code, message, and data\", function () {",
					"        pm.expect(jsonData).to.have.property(\"success\");",
					"        pm.expect(jsonData).to.have.property(\"code\");",
					"        pm.expect(jsonData).to.have.property(\"message\");",
					"        pm.expect(jsonData).to.have.property(\"data\");",
					"    });",
					"    pm.test(\"Data is an array\", function () {",
					"        pm.expect(jsonData.data).to.be.an(\"array\");",
					"    });",
					"    pm.test(\"Each product has required fields\", function () {",
					"        jsonData.data.forEach(function(product) {",
					"            pm.expect(product).to.have.property(\"nombre\");",
					"            pm.expect(product).to.have.property(\"descripcion\");",
					"            pm.expect(product).to.have.property(\"informacion_extra\");",
					"            pm.expect(product).to.have.property(\"nombre_categoria\");",
					"            pm.expect(product).to.have.property(\"nombre_temporada\");",
					"            pm.expect(product).to.have.property(\"imagenes\");",
					"            pm.expect(product.imagenes).to.be.an(\"array\");",
					"        });",
					"    });",
					"}",
